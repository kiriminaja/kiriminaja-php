{
  "name": "kiriminaja/kiriminaja-php",
  "description": "PHP clients for KiriminAja API",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "kiriminaja",
    "kirimin aja"
  ],
  "authors": [
    {
      "name": "Daewu Bintara",
      "email": "daewu.bintara1996@gmail.com"
    }
  ],
  "homepage": "https://kiriminaja.com",
  "type": "library",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.2.0",
    "guzzlehttp/guzzle": ">=6.0",
    "ext-json": "*",
    "rakit/validation": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "KiriminAja\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KiriminAja\\": [
        "tests/",
        "tests/Xendit/"
      ]
    }
  },
  "require-dev": {
    "mockery/mockery": "dev-master",
    "phpunit/phpunit": "^9.5.4"
  }
}
